{
    "name": "steos/quickcheck",
    "description": "a generative testing library",
    "license": "BSD-3-Clause",
    "require": {
        "php-64bit": ">=7.3.0",
        "symfony/console": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {"QuickCheck\\": "src/QuickCheck/"}
    },
    "autoload-dev": {
        "psr-4": {"QuickCheck\\": "test/QuickCheck/"}
    },
    "suggest": {
        "ext-gmp": "*"
    },
    "bin": [
        "bin/quickcheck"
    ]
}
